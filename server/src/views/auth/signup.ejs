<div class="row">
  <form action="./auth/signup" method="post" class="col s12">
    <div class="row">
      <div class="input-field col s12">
        <input id="emailInput" type="email" name="email" required class="validate">
        <label for="emailInput">Email</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="nameInput" type="text" name="name" required class="validate">
        <label for="nameInput">Name</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="usernameInput" type="text" name="username" required class="validate">
        <label for="usernameInput">Username</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="passwordInput" type="password" name="password" required class="validate">
        <label for="passwordInput">Password</label>
      </div>
    </div>
    <div class="row">
      <button class="waves-effect waves-teal btn" type="submit">Sign up</button>
    </div>
  </form>
</div>
